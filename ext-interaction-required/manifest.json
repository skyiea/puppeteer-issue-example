{
    "name": "Notification on Install",
    "description": "Shows notification on installation",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": [
        "notifications"
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}